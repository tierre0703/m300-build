
config interface 'wan'
	option enabled '0'
	list track_ip '180.76.76.76'
	list track_ip '119.29.29.29'
	option reliability '1'
	option count '1'
	option timeout '2'
	option interval '5'
	option down '3'
	option up '3'

config interface 'wan1'
	option enabled '0'
	list track_ip '180.76.76.76'
	list track_ip '119.29.29.29'
	option reliability '1'
	option count '1'
	option timeout '2'
	option interval '5'
	option down '3'
	option up '3'

config member 'wan_member'
	option interface 'wan'
	option metric '1'
	option weight '1'

config member 'wan1_member'
	option interface 'wan1'
	option metric '1'
	option weight '1'

config policy 'balanced'

config rule 'https'
	option sticky '1'
	option dest_port '443'
	option proto 'tcp'
	option use_policy 'balanced'
	
config rule 'default_rule'
	option dest_ip '0.0.0.0/0'
	option use_policy 'balanced'

