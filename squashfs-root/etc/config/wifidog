config wifidog 'wifidog'
	option enabled '0'
	option gateway_id '0'
        option gateway_interface 'br-lan'
	option gateway_externalface 'eth0'
	option gateway_address '172.16.0.1'
	option html_message_file 'wifidog-msg.html'
	option hostname 'c.weifeinet.com'
	option httpport '80'
	option path '/'
	option daemon '1'
	option gateway_port '2060'
	option httpd_name 'wifidog'
	option httpd_max_conn '32'
	option httpd_realm 'WiFiDog'
	option httpd_username 'unset'
	option httpd_password 'unset'
	option check_interval '300'
	option client_timeout '30'
	list popular_servers 'kernel.org,ieee.org'
	list trusted_mac_list ''
	list trusted_web_list ''
#	list trusted_web_list 'weixin.qq.com'
#	list trusted_web_list 'short.weixin.qq.com'
#	list trusted_web_list 'long.weixin.qq.com'
#	list trusted_web_list 'szsupport.weixin.qq.com'
#	list trusted_web_list 'my.weifeinet.com'
#	list trusted_web_list 'szextshort.weixin.qq.com'
#	list trusted_web_list 'szshort.weixin.qq.com'
	list firewall_rule_set ''
	list firewall_rule ''